<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_web_server_menu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="mx.edu.utng.wsmovie.WebServerMenu">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/bt_firs_ws"
        android:text="@string/ws_movie"
        android:fontFamily="casual"
        android:textSize="15dp"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/bt_second_ws"
        android:text="@string/ws_document"
        android:fontFamily="casual"
        android:textSize="15dp"
        android:layout_below="@id/bt_firs_ws"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/bt_third_ws"
        android:text="@string/third_wws"
        android:fontFamily="casual"
        android:textSize="15dp"
        android:layout_below="@id/bt_second_ws"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/bt_fourth_ws"
        android:text="@string/fourth_ws"
        android:fontFamily="casual"
        android:textSize="15dp"
        android:layout_below="@id/bt_third_ws"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/bt_fifth_ws"
        android:text="@string/fifth_ws"
        android:fontFamily="casual"
        android:textSize="15dp"
        android:layout_below="@id/bt_fourth_ws"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/bt_sixth_ws"
        android:text="@string/sixth_ws"
        android:fontFamily="casual"
        android:textSize="15dp"
        android:layout_below="@id/bt_fifth_ws"/>


</RelativeLayout>
